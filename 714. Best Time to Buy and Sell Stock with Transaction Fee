class Solution {
        public int maxProfit(int[] prices, int fee) {
            int length = prices.length;
            int buy = -prices[0];
            int sell = 0;

            for (int i = 1; i < prices.length; i++) {
                int curr = prices[i];
                buy = Math.max(buy, sell-curr);
                sell = Math.max(sell, buy +curr-fee);
            }
            return sell;
        }
    }
